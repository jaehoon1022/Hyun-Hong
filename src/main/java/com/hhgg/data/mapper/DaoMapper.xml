<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hhgg.data.dao.SummonerDao">
    <select id="findByName" resultType="com.hhgg.data.dto.SummonerDTO">
        SELECT * FROM summoner WHERE NAME = #{name};
    </select>
    <insert id="saveName" parameterType="com.hhgg.data.dto.SummonerDTO">
        INSERT INTO SUMMONER VALUES(
           #{accountId},
           #{profileIconId},
           #{revisionDate},
           #{name},
           #{id},
           #{puuid},
           #{summonerLevel});
    </insert>
</mapper>